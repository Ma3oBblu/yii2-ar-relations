{
    "name": "sorokinmedia/yii2-ar-relations",
    "description": "AR relations for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "active record", "AR"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ruslan Gilyazetdinov",
            "email": "ma3obblu@gmail.com",
            "homepage": "https://www.facebook.com/Ma3oBblu"
        }
    ],
    "support": {
        "issues": "https://github.com/sorokinmedia/yii2-ar-relations/issues?state=open",
        "source": "https://github.com/sorokinmedia/yii2-ar-relations"
    },
    "require": {
        "yiisoft/yii2": ">=2.0.30"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "sorokinmedia\\ar_relations\\": "src/",
            "sorokinmedia\\ar_relations\\examples\\": "examples/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
