{
    "name": "ma3obblu/yii2-ar-relations",
    "description": "AR relations for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "active record", "AR"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ruslan Gilyazetdinov",
            "email": "ma3obblu@gmail.com",
            "homepage": "https://vk.com/ma3obblu"
        }
    ],
    "support": {
        "issues": "https://github.com/Ma3oBblu/yii2-ar-relations/issues?state=open",
        "source": "https://github.com/Ma3oBblu/yii2-ar-relations"
    },
    "require": {
        "yiisoft/yii2": ">=2.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "ma3obblu\\ar_relations\\": "src/",
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}